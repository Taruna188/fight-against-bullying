{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "target": "es2016",
    "module": "ES2022",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "$lib":["./src/lib"],
      "$lib/*":["./src/lib/*"]
    }
  }
}
